{
  "name": "smartspace",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "react"
  ],
  "description": "",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "antd": "~3.26.8",
    "classnames": "~2.2.6",
    "core-js": "~3.6.4",
    "echarts": "~4.6.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "4.17.15",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-intl": "~3.11.0",
    "react-redux": "~7.1.3",
    "react-router-dom": "~5.1.2",
    "redux": "~4.0.5",
    "redux-actions": "~2.6.5",
    "redux-thunk": "~2.3.0",
    "reselect": "~4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "~7.8.3",
    "@babel/plugin-proposal-optional-chaining": "~7.8.3",
    "alpha-scripts": "~2.0.5-beta",
    "babel-plugin-import": "~1.13.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-plugin-prettier": "~3.1.2",
    "flow-bin": "~0.117.0",
    "flow-typed": "~2.6.2",
    "lint-staged": "~10.0.7",
    "mockjs": "1.1.0",
    "prettier": "~1.19.1",
    "rimraf": "~3.0.1"
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint \"src/**/*.@(js|jsx)\" -f node_modules/eslint-html-reporter/reporter.js -o eslint_report.html && exit 0",
    "test": "alpha-scripts test --env=jsdom",
    "start": "alpha-scripts start",
    "build": "alpha-scripts build",
    "clean-flow-type": "rimraf flow-typed/*",
    "flow-typed-install": "npm run clean-flow-type && flow-typed install",
    "flow": "flow check",
    "postinstall": "npm run flow-typed-install  && npm start",
    "precommit": "lint-staged",
    "lint-fix": "eslint src/**/*.js --fix"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ]
}
